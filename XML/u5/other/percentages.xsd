<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="percentage">
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value="(\d{1,2}|100)%"/>
        </xs:restriction>
      </xs:simpleType>
  </xs:element>
</xs:schema>
